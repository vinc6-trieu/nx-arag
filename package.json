  "license": "MIT",
  "scripts": {
    "build:auth": "yarn nx build auth",
    "dev:auth": "yarn nx serve auth",
    "start:auth": "node packages/services/auth/dist/main.js",
    "clean": "rimraf dist node_modules",
    "lint": "yarn nx lint",
    "lint:fix": "yarn nx lint --fix",
    "format": "prettier -c \"**/*.{ts,js,json,md}\"",
    "format:fix": "prettier --write \"**/*.{ts,js,json,md}\"",
    "test": "yarn nx test",
    "test:watch": "yarn nx test --watch",
    "typecheck": "yarn nx run chat:typecheck",
    "db:migrate:auth": "yarn nx run auth:prisma-migrate",
    "db:reset:auth": "yarn nx run auth:prisma-reset --force",
    "db:deploy:auth": "yarn nx run auth:prisma-deploy",
    "db:generate:auth": "yarn nx run auth:prisma-generate",
    "affected:lint": "yarn nx affected --target=lint     --base=origin/main",
    "affected:test": "yarn nx affected --target=test     --base=origin/main",
    "affected:build": "yarn nx affected --target=build    --base=origin/main"
  },
  "private": true,
