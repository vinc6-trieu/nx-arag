{
  "name": "@nx-arag/ingest-svc",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "ingest-svc",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "webpack-cli build",
          "args": [
            "node-env=production"
          ]
        },
        "configurations": {
          "development": {
            "args": [
              "node-env=development"
            ]
          }
        }
      },
      "serve": {
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "ingest-svc:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "ingest-svc:build:development"
          },
          "production": {
            "buildTarget": "ingest-svc:build:production"
          }
        }
      },
      "lint": {
        "executor": "nx:run-commands",
        "options": {
          "command": "eslint \"apps/ingest-svc/src/**/*.ts\""
        }
      },
      "test": {
        "options": {
          "passWithNoTests": true
        }
      },
      "prisma-deploy": {
        "executor": "@nx-tools/nx-prisma:deploy",
        "options": {
          "schema": "apps/ingest-svc/prisma/schema.prisma"
        }
      },
      "prisma-format": {
        "executor": "@nx-tools/nx-prisma:format",
        "options": {
          "schema": "apps/ingest-svc/prisma/schema.prisma"
        }
      },
      "prisma-generate": {
        "executor": "@nx-tools/nx-prisma:generate",
        "options": {
          "schema": "apps/ingest-svc/prisma/schema.prisma"
        }
      },
      "prisma-migrate": {
        "executor": "@nx-tools/nx-prisma:migrate",
        "options": {
          "schema": "apps/ingest-svc/prisma/schema.prisma"
        }
      },
      "prisma-pull": {
        "executor": "@nx-tools/nx-prisma:pull",
        "options": {
          "schema": "apps/ingest-svc/prisma/schema.prisma"
        }
      },
      "prisma-push": {
        "executor": "@nx-tools/nx-prisma:push",
        "options": {
          "schema": "apps/ingest-svc/prisma/schema.prisma"
        }
      },
      "prisma-reset": {
        "executor": "@nx-tools/nx-prisma:reset",
        "options": {
          "schema": "apps/ingest-svc/prisma/schema.prisma"
        }
      },
      "prisma-resolve": {
        "executor": "@nx-tools/nx-prisma:resolve",
        "options": {
          "schema": "apps/ingest-svc/prisma/schema.prisma"
        }
      },
      "prisma-seed": {
        "executor": "@nx-tools/nx-prisma:seed",
        "options": {
          "schema": "apps/ingest-svc/prisma/schema.prisma"
        }
      },
      "prisma-status": {
        "executor": "@nx-tools/nx-prisma:status",
        "options": {
          "schema": "apps/ingest-svc/prisma/schema.prisma"
        }
      },
      "prisma-studio": {
        "executor": "@nx-tools/nx-prisma:studio",
        "options": {
          "schema": "apps/ingest-svc/prisma/schema.prisma"
        }
      },
      "prisma-validate": {
        "executor": "@nx-tools/nx-prisma:validate",
        "options": {
          "schema": "apps/ingest-svc/prisma/schema.prisma"
        }
      }
    }
  }
}
